<experiments>
  <experiment name="rp1.2" repetitions="16" runMetricsEveryStep="true">
    <setup>
        setup
        export-risk
        export-opinions
    </setup>
  <go>go</go>
  <final>
        export-risk
        export-opinions
        show (word "Time elapsed:" timer)
    </final>
  <timeLimit steps="30600" />
  <metric>t1</metric>
  <metric>t4 "median" 1</metric>
  <metric>t4 "standard-deviation" 1</metric>
  <metric>t4 "mean" 2</metric>
  <metric>t4 "median" 2</metric>
  <metric>t4 "standard-deviation" 2</metric>
  <metric>t4 "mean" 3</metric>
  <metric>t4 "median" 3</metric>
  <metric>soc-counter</metric>
  <metric>soc-online-counter</metric>
  <metric>rec-counter</metric>
  <metric>min [ hours-to-recruit ] of citizens</metric>
  <metric>mean [ hours-to-recruit ] of citizens</metric>
  <metric>count links</metric>
  <metric>fail-activity-counter</metric>
  <metric>police-actions-counter</metric>
  <enumeratedValueSet variable="high-risk-employed">
      <value value="&quot;no intervention&quot;" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="total-citizens">
      <value value="10000" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="alpha">
      <value value="0.1" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="population-employed-%">
      <value value="10" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="activity-radius">
      <value value="5" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="activity-debug?">
      <value value="false" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="work-socialization-probability">
      <value value="0.1" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="number-workers-per-community-center">
      <value value="1" />
      <value value="5" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="links-cap-mean">
      <value value="10" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="male-ratio">
      <value value="50" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="cpo-%">
      <value value="0" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="recruit-hours-threshold">
      <value value="200" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="activity-value-update">
      <value value="0.5" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="scenario">
      <value value="&quot;neukolln&quot;" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="community-side-length">
      <value value="35" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="radicalization-percentage">
      <value value="5.6" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="talk-effect-size">
      <value value="0.04" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="socialize-probability">
      <value value="0.1" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="criminal-history-percent">
      <value value="20" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="num-recruiters">
      <value value="15" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="mean-hours-worked-recruiter">
      <value value="6" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="running-plan">
      <value value="&quot;rp1.2-10K-long&quot;" />
    </enumeratedValueSet>
  <enumeratedValueSet variable="opinion-dumps-every">
      <value value="5000" />
    </enumeratedValueSet>
  </experiment>
</experiments>
